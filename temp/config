# Configuration for your main server
Host your-server
  HostName 66.179.189.159
  User your_username
  IdentityFile ~/.ssh/id_rsa
  IdentitiesOnly yes

# Configuration for GitHub
Host github.com
  AddKeysToAgent yes
  UseKeychain yes
  IdentityFile ~/.ssh/id_ed25519

#This file is to be placed at '/.ssh/config'